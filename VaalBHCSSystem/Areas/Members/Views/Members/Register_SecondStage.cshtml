        var iCurrentIndex = 0;
        $.each(_MainDataStore.FamilyMembers, function (i, MemberAffiliate) {
            if (i == iIndex) {
                iCurrentIndex = i;
            }
        });
        _MainDataStore.FamilyMembers.splice(iCurrentIndex, 1);
        buildMemberAfilliationTable();
    }
